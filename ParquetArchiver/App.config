<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <!-- For more information on Entity Framework configuration, visit http://go.microsoft.com/fwlink/?LinkID=237468 -->
    <section name="entityFramework" type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework, Version=6.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
  </configSections>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
  </startup>
  <appSettings>
    <!--Local Storage Settings-->
    <add key="LocalArchiveDirectory" value="F:\ATSPMStorage\" />

    <!--1 to use the StartDate and EndDate settings-->
    <!--0 to use DaysToKeep and DaysBackToCheck settings-->
    <add key="UseStartAndEndDates" value="0" />

    <!--format dates: mm/dd/yyyy-->
    <add key="StartDate" value="01/01/2018" />
    <add key="EndDate" value="07/01/2018" />

    <!--How many days ago to run and copy to archive-->
    <add key="DaysAgoToRun" value="3" />
    <!--How many days back from DaysAgoToRun to check for a missing day-->
    <!--Set to 0 to disable feature-->
    <add key="DaysBackToCheck" value="30" />

    <!--1 to send an email after a run-->
    <add key="SendCompletionEmail" value="1" />

    <add key="SmtpServer" value="mailrelay.dot.ga.gov" />
    <add key="FromEmail" value="ParquetArchiver@dot.ga.gov" />
    <!--1 to use SSL-->
    <add key="UseSSL" value="0" />
    <!--A password may be required if SSL is enabled-->
    <add key="FromEmailPassword" value="" />
    <!--Separate multiple addresses with a semicolon-->
    <add key="EmailList" value="mark.wood@kimley-horn.com" />
  </appSettings>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="System.Buffers" publicKeyToken="cc7b13ffcd2ddd51" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-4.0.3.0" newVersion="4.0.3.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
  <entityFramework>
    <defaultConnectionFactory type="System.Data.Entity.Infrastructure.LocalDbConnectionFactory, EntityFramework">
      <parameters>
        <parameter value="mssqllocaldb" />
      </parameters>
    </defaultConnectionFactory>
    <providers>
      <provider invariantName="System.Data.SqlClient" type="System.Data.Entity.SqlServer.SqlProviderServices, EntityFramework.SqlServer" />
    </providers>
  </entityFramework>
</configuration>